{
  "extends": ["../../.eslintrc", "@react-native-community", "eslint-config-prettier"],
  "plugins": ["prettier", "react-native"],
  "ignorePatterns": [".eslintrc.js", "src/infrastructure/inversify.config.ts"],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "jsx": true,
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}
